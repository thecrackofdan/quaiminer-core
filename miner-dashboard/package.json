{
  "name": "quaiminer-core-dashboard",
  "version": "1.0.0",
  "description": "QuaiMiner CORE - Production-ready real-time dashboard for solo mining Quai Network with your own node",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "NODE_ENV=development node server.js",
    "dev:unix": "NODE_ENV=development node server.js",
    "test": "node -c server.js && echo '✅ Syntax check passed'",
    "verify": "node -c server.js && npm audit --audit-level=moderate && echo '✅ Verification complete'",
    "docker:build": "docker build -t quaiminer-dashboard .",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f"
  },
  "keywords": [
    "quaiminer-core",
    "quaiminer",
    "mining",
    "dashboard",
    "quai",
    "quai-network",
    "cryptocurrency",
    "monitoring",
    "blockchain",
    "solo-mining",
    "amd-gpu",
    "progpow"
  ],
  "author": "thecrackofdan",
  "license": "MIT",
  "dependencies": {
    "express": "^4.21.2",
    "cors": "^2.8.5",
    "better-sqlite3": "^9.2.2",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "express-validator": "^7.0.1",
    "chart.js": "^4.4.0",
    "pdfkit": "^0.14.0",
    "csv-writer": "^1.6.0"
  },
  "devDependencies": {},
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.0.0"
  },
  "os": [
    "win32",
    "linux",
    "darwin"
  ],
  "cpu": [
    "x64",
    "arm64"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/thecrackofdan/quaiminer-core.git",
    "directory": "miner-dashboard"
  },
  "bugs": {
    "url": "https://github.com/thecrackofdan/quaiminer-core/issues"
  },
  "homepage": "https://github.com/thecrackofdan/quaiminer-core#readme"
}
